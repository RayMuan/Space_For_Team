<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Project_4team</display-name>
<!--   주소가 입력되어있지 않으면 찾는 파일  welcome-file -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
<!--   *.me 가상주소 -> 주소매핑 처리 파일로 연결 -> 처리담당자(서블릿) -->
 <servlet>
	<servlet-name>BookingFrontController</servlet-name>
	<servlet-class>com.itwillbs.Booking.action.BookingFrontController</servlet-class>
  </servlet>

  <servlet>
	<servlet-name>MemberFrontController</servlet-name>
	<servlet-class>com.itwillbs.member.action.MemberFrontController</servlet-class>
  </servlet>
  
 
  
<!--  *.me인 파일을 MemberFrontController라는 서블릿이 처리할 것이고 이 서블릿은  -->
<!--  com.itwillbs.member.action.MemberFrontController 에 있음 -->
   
  <servlet-mapping>
  	<servlet-name>BookingFrontController</servlet-name>
  	<url-pattern>*.bk</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>MemberFrontController</servlet-name>
  	<url-pattern>*.me</url-pattern>
  </servlet-mapping>
</web-app>